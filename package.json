    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.2.2",
    "@testcontainers/kafka": "^10.2.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
