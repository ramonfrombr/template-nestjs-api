  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=local nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand",
    "test:cov": "yarn test --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:run": "yarn typeorm migration:run -d ./src/infrastructure/db/data-source.ts",
    "migration:show": "yarn typeorm migration:show -d ./src/infrastructure/db/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -d ./src/infrastructure/db/data-source.ts",
    "migration:create": "yarn typeorm migration:create"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.2",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.2",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/platform-express": "^10.2.2",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.5.1",
    "cache-manager": "^5.2.3",
    "helmet": "^7.0.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
